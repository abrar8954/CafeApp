<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abrarsardar.FriendCafe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.FriendCafe">
        <activity android:name="com.abrarsardar.FriendCafe.Book_Table"></activity>
        <activity android:name="com.abrarsardar.FriendCafe.Home_Delivery" />
        <activity android:name="com.abrarsardar.FriendCafe.Pancakes" />
        <activity android:name="com.abrarsardar.FriendCafe.Menu" />
        <activity android:name="com.abrarsardar.FriendCafe.Sign_In" />
        <activity android:name="com.abrarsardar.FriendCafe.Sign_Up" />
        <activity android:name="com.abrarsardar.FriendCafe.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.abrarsardar.FriendCafe.MainActivity" />
    </application>

</manifest>